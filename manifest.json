{
    "name" : "InstagramCatch",
    "short_name" : "iC",
    "version" : "0.1.1",
    "content_scripts" : [
      {
        "matches" : [
          "https://www.instagram.com/*"
        ],
        "js" : ["content.js"]
      }
    ],
    "permissions" : ["tabs","storage"],
    "browser_action" : {
      "default_icon" : "icon_128.png",
      "default_popup" : "popup.html"
    },
    "manifest_version" : 2
}
